local mainUI = ui:Init(game:GetService("CoreGui"))
local homeTab = mainUI:AddTab("Visuals")
local autoTab = mainUI:AddTab("Cheats")
local farmSection = homeTab:AddSection("Arsenal")
local farmMobs = farmSection:AddToggle("Chams", false, function(value)
	if value then
		
	end
end)
